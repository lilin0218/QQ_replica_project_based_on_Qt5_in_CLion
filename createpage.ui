   <string notr="true">/* 顶部搜索栏背景 */
QWidget#TopBar {
    background-color: rgba(255, 255, 255, 0.05);
    border-bottom: 1px solid rgba(255,255,255,0.2);
}

/* QToolButton 搜索图标按钮 */
QToolButton#toolButton_search {
    background-color: transparent;
    border: none;
    qproperty-iconSize: 18px;
}
QToolButton#toolButton_search:hover {
    background-color: rgba(255, 255, 255, 0.1);
    border-radius: 6px;
}

/* QLineEdit 搜索框 */
QLineEdit#lineEdit_search {
    background-color: rgba(255, 255, 255, 0.1);
    border: 1px solid rgba(255, 255, 255, 0.3);
    border-radius: 6px;
    padding: 6px 10px;
    color: white;
}
QLineEdit#lineEdit_search:focus {
    border: 1px solid #1abc9c;
    background-color: rgba(255, 255, 255, 0.15);
}

/* QLabel 标签 */
QLabel {
    font-weight: bold;
}
QLabel#label_more {
    color: #1abc9c;
    font-size: 13px;
}
QLabel#label_more:hover {
    text-decoration: underline;
}

/* QPushButton 通用样式 */
QPushButton {
    background-color: qlineargradient(x1:0, y1:0, x2:1, y2:0,
        stop:0 #3498db, stop:1 #2980b9);
    border: none;
    border-radius: 6px;
    padding: 6px 14px;
    color: white;
    font-weight: bold;
}
QPushButton:hover {
    background-color: #1abc9c;
}
QPushButton:pressed {
    background-color: #16a085;
}

/* 取消按钮特化样式 */
QPushButton#pushButton_cancel {
    background-color: transparent;
    border: 1px solid #bdc3c7;
    color: #bdc3c7;
}
QPushButton#pushButton_cancel:hover {
    background-color: rgba(255,255,255,0.1);
    color: white;
    border: 1px solid white;
}

/* 分割线 */
QFrame[orientation=&quot;Horizontal&quot;] {
    color: rgba(255, 255, 255, 0.2);
    background-color: rgba(255, 255, 255, 0.2);
    max-height: 1px;
}

/* widget_menu 区域，可用于列表容器背景 */
QWidget#widget_menu {
    background-color: rgba(255, 255, 255, 0.03);
    border-radius: 8px;
}

/* QToolButton 动效，去除 transition */
QToolButton#toolButton_search:hover {
    background-color: rgba(255, 255, 255, 0.1);
    border-radius: 6px;
}

/* QLabel 鼠标悬浮效果 */
QLabel#label_more:hover {
    color: #16a085; /* 更改为悬浮时颜色 */
}

/* QLineEdit 和 QPushButton 鼠标悬浮效果 */
QPushButton:hover, QLineEdit:hover {
    background-color: rgba(255, 255, 255, 0.15);
    border: 1px solid #1abc9c;
}

/* QPushButton 选中时的效果 */
QPushButton:checked {
    background-color: #16a085;
    border: none;
}
</string>
